  rownames_to_column(var = 'variable') %>%
  filter(grepl('Dyspl', variable)) %>%
  mutate(HR = exp(coef), 
         HR_lower = exp(coef - qnorm(0.975) * `se(coef)`),
         HR_upper = exp(coef + qnorm(0.975) * `se(coef)`)) %>% 
  select(variable, HR, HR_lower, HR_upper, `Pr(>|z|)`)
out2s_eth_5yr # Results for Dysplasiax across ethnicities, reference is 'Low grade'

