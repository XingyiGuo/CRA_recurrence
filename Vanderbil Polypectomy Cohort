library(readxl)
library(tidyverse)
library(survival)
library(gridExtra)
library(gt)
library(gtsummary)
library(grid)
library(ggpubr) 
library(Cairo)



# Read data
data <- read_excel("Path")

# Data preprocessing
df <- data %>%
  select(
    person_id, Gender, ethnicity, obesity, `Polyp Onset Stratification`,
    `Adenoma Type`, `Recurrence Status`,
    birth_datetime, polypectomy_date_1, polypectomy_date_2, `Polyp Type`, Dysplasia,
    `Number of Polyp`, `Size of Polyp (mm)`, `First Recurrence Date`, Number_of_Recurrence, `NSAIDs`, `Aspirin`
  ) %>%
  mutate(
    recurrence_time_months = ifelse(
      !is.na(polypectomy_date_2),
      as.numeric(as.Date(polypectomy_date_2) - as.Date(polypectomy_date_1)) / 30.44,
      NA
    ),
    `Recurrence Status` = ifelse(
      is.na(recurrence_time_months) | recurrence_time_months < 6,
      "Not Recurrence",
      "Recurrence"
    ),
    event = ifelse(`Recurrence Status` == "Recurrence", 1, 0),
    age_in = as.numeric(as.Date(polypectomy_date_1) - as.Date(birth_datetime)) / 365.25,
    age_out = as.numeric(as.Date(`First Recurrence Date`) - as.Date(birth_datetime)) / 365.25,
    age_grp = cut(age_in, breaks = c(0, 50, 60, 70, 96), labels = 1:4)
  ) %>%
  filter(age_out > age_in & age_in >= 18 & ethnicity != "Other" & !is.na(`Adenoma Type`)) %>%
  mutate(
    Gender = relevel(factor(Gender), ref = "Female"),
    ethnicity = relevel(factor(ethnicity), ref = "Non-Hispanic White"),
    `Polyp Onset Stratification` = relevel(factor(`Polyp Onset Stratification`), ref = "51-75"),
    Dysplasia = relevel(factor(Dysplasia), ref = "Low grade"),
    `Number of Polyp` = relevel(factor(`Number of Polyp`), ref = "<3"),
    `Size of Polyp (mm)` = relevel(factor(`Size of Polyp (mm)`), ref = "<10"),
    `Adenoma Type` = relevel(factor(`Adenoma Type`), ref = "Low Risk"),
    `Polyp Type` = relevel(factor(`Polyp Type`), ref = "Tubular")
  ) %>%
  mutate(
    sex = ifelse(Gender == "Female", 0, 1),
    race = factor(ifelse(
      ethnicity == "Non-Hispanic White", 1,
      ifelse(ethnicity == "Non-Hispanic Black", 2,
             ifelse(ethnicity == "Hispanic", 3,
                    ifelse(ethnicity == "Asian or Pacific Islander", 4, NA)
             )
      )
    )),
    `Adenoma Type` = ifelse(`Adenoma Type` == "Low Risk", 0, 1),
    polyp_strat = factor(`Polyp Onset Stratification`)
  )

# Calculate follow-up years
df$fu_yrs <- df$age_out - df$age_in

# Create additional factor variables for analysis with reference details
df <- within(df, {
  PolypType <- as.factor(ifelse(`Polyp Type` == 'Tubular', 7,
                                ifelse(`Polyp Type` == 'Mixed', 6,
                                       ifelse(`Polyp Type` == 'Non-neoplastic', 5,
                                              ifelse(`Polyp Type` == 'Not defined', 4,
                                                     ifelse(`Polyp Type` == 'Serrated', 3,
                                                            ifelse(`Polyp Type` == 'Tubulovillous', 2, 1))))))) # Reference: "Tubular" (coded as 7)
  Dysplasiax <- as.factor(ifelse(Dysplasia == 'Low grade', 1, 0)) # Reference: "Low grade" (coded as 1)
  PolypNum <- as.factor(ifelse(`Number of Polyp` == '<3', 2,
                               ifelse(`Number of Polyp` == '≥3', 1, 0))) # Reference: "<3" (coded as 2)
  PolypSize <- as.factor(ifelse(`Size of Polyp (mm)` == '<10', 2,
                                ifelse(`Size of Polyp (mm)` == '≥10', 1, 0))) # Reference: "<10" (coded as 2)
  Gender <- as.factor(ifelse(Gender == 'Female', 0, 1)) # Reference: "Female" (coded as 0)
  AdenomaType <- as.factor(ifelse(`Adenoma Type` == 'Low Risk', 0, 1)) # Reference: "Low Risk" (coded as 0)
  Ethnicity <- as.factor(ifelse(ethnicity == 'Non-Hispanic White', 1,
                                ifelse(ethnicity == 'Non-Hispanic Black', 2,
                                       ifelse(ethnicity == 'Hispanic', 3,
                                              ifelse(ethnicity == 'Asian or Pacific Islander', 4, NA))))) # Reference: "Non-Hispanic White" (coded as 1)
  PolypOnsetStrat <- as.factor(ifelse(`Polyp Onset Stratification` == '51-75', 1,
                                      ifelse(`Polyp Onset Stratification` == '<51', 2,
                                             ifelse(`Polyp Onset Stratification` == '>75', 3, NA)))) # Reference: "51-75" (coded as 1)
  # Obesity: Not explicitly relevelled, coded as obesity (0 = non-obese, 1 = obese) → Reference: "non-obese" (assumed 0)
})

# Split data for time-dependent analysis and filter for ≤5 years
dfs <- survSplit(Surv(fu_yrs, event) ~ ., data = df, cut = c(5, 10), end = 'tstop', episode = "tgroup")
dfs_5yr <- dfs %>% filter(tgroup == 1)

# Check dimensions and recurrence status
dim(dfs_5yr)
dfs_5yr %>% count(`Recurrence Status`)


dfs_5yr <- dfs_5yr %>%
  mutate(followup_time = tstop - tstart)

summary(dfs_5yr$followup_time)
range(dfs_5yr$followup_time)



# Define color palette
color_palette <- c("#1F78B4", "#D55E00", "#33A02C", "#E6AB02", "#6A3D9A", "grey8", "red")

# Create custom theme with horizontal legend at the bottom
custom_theme <- theme(
  panel.grid.major.y = element_line(color = "grey80", linetype = 1),
  panel.grid.major.x = element_blank(),
  panel.grid.minor = element_blank(),
  panel.background = element_blank(),
  plot.title = element_text(size = rel(1.8), hjust = 0.5, face = "bold"),
  axis.title = element_text(size = rel(1.5)),
  axis.text = element_text(size = rel(1.3)),
  legend.position = "bottom",         # Legend at bottom
  legend.direction = "horizontal",    # Horizontal layout
  legend.title = element_blank(),
  legend.text = element_text(size = rel(1.1)),
  legend.background = element_blank(),
  legend.key.height = unit(1.2, "lines"),
  plot.margin = margin(5, 5, 10, 4, "mm")  # Increased bottom margin for legend space
)

# Models (unchanged)
mod1 <- coxph(Surv(fu_yrs, event) ~ PolypType + ethnicity + sex + obesity + NSAIDs + Aspirin + age_in, data = df)
newdd1 <- data.frame(
  PolypType = as.factor(c(1:7)), 
  age_in = 60, 
  ethnicity = 'Non-Hispanic White', 
  sex = 0, 
  obesity = 0, 
  NSAIDs = 0, 
  Aspirin = 0
)
polyp_types <- c('Villous', 'Tubulovillous', 'Serrated', 'Not defined', 'Non-neoplastic', 'Mixed', 'Tubular')

mod2 <- coxph(Surv(fu_yrs, event) ~ Dysplasiax + ethnicity + sex + obesity + NSAIDs + Aspirin + age_in, data = df)
newdd2 <- data.frame(
  Dysplasiax = as.factor(c(0, 1)), 
  age_in = 60, 
  ethnicity = 'Non-Hispanic White', 
  sex = 0, 
  obesity = 0, 
  NSAIDs = 0, 
  Aspirin = 0
)
dysplasia_types <- c('High grade', 'Low grade')

mod3 <- coxph(Surv(fu_yrs, event) ~ PolypNum + ethnicity + sex + obesity + NSAIDs + Aspirin + age_in, data = df)
newdd3 <- data.frame(
  PolypNum = as.factor(c(0, 1, 2)), 
  age_in = 60, 
  ethnicity = 'Non-Hispanic White', 
  sex = 0, 
  obesity = 0, 
  NSAIDs = 0, 
  Aspirin = 0
)
polyp_numbers <- c('Unknown', '≥3', '<3')

mod4 <- coxph(Surv(fu_yrs, event) ~ PolypSize + ethnicity + sex + obesity + NSAIDs + Aspirin + age_in, data = df)
newdd4 <- data.frame(
  PolypSize = as.factor(c(0, 1, 2)), 
  age_in = 60, 
  ethnicity = 'Non-Hispanic White', 
  sex = 0, 
  obesity = 0, 
  NSAIDs = 0, 
  Aspirin = 0
)
polyp_sizes <- c('Unknown', '≥10', '<10')


# Plots for 5-year follow-up
# For Polyp Type
sur1 <- survfit(mod1, newdata = newdd1)
df_plot1 <- data.frame(time = sur1$time)
for (i in 1:7) {
  df_plot1[, i + 1] <- sur1$surv[, i]
}
names(df_plot1) <- c('time', polyp_types)
df_plot1_long <- df_plot1 %>% 
  pivot_longer(cols = -time, names_to = 'polyp', values_to = 'surv')

plot1 <- ggplot(df_plot1_long, aes(x = time, y = 1 - surv, color = polyp)) + 
  geom_line(linewidth = 1) +
  scale_color_manual(values = color_palette) +
  scale_x_continuous(limits = c(0, 5), breaks = seq(0, 5, 1)) +
  scale_y_continuous(limits = c(0, 1), breaks = seq(0, 1, 0.2)) +
  labs(title = "(A) Polyp Type", x = "Time to Recurrence (years)", y = "Probability of Recurrence", color = "Polyp Type") +
  theme_classic() +
  custom_theme +
  # Add table with placeholder values
  annotate("text", x = 0.2, y = 0.88, label= "                          HR      95% CI        p-value", hjust = 0, size = 4, fontface = "bold") +
  annotate("text", x = 0.2, y = 0.78, label = bquote("Villous                2.4        2.18, 2.65      "~5.26 %*% 10^{-70}), hjust = 0, size = 4) +
  annotate("text", x = 0.2, y = 0.71, label = bquote("Tubulovillous      2.15      1.96, 2.36     "~ 1.40 %*% 10^{-59}), hjust = 0,  size = 4) +
  annotate("text", x = 0.2, y = 0.64, label = bquote("Serrated             1.31      1.19, 1.45     "~5.42 %*% 10^{-8}), hjust = 0, size = 4) +
  annotate("text", x = 0.2, y = 0.57, label = bquote("Not defined        0.51      0.47, 0.56     "~4.31 %*% 10^{-59}), hjust = 0,  size = 4) +
  annotate("text", x = 0.2, y = 0.5,  label = bquote("Non-neoplastic   0.59     0.54,  0.64     "~1.58 %*% 10^{-38}), hjust = 0,  size = 4) +
  annotate("text", x = 0.2, y = 0.43, label = bquote("Mixed                 1.36      1.29, 1.43     "~3.41 %*% 10^{-30}), hjust = 0,  size = 4)

# For Dysplasia
sur2 <- survfit(mod2, newdata = newdd2)
df_plot2 <- data.frame(time = sur2$time)
for (i in 1:2) {
  df_plot2[, i + 1] <- sur2$surv[, i]
}
names(df_plot2) <- c('time', dysplasia_types)
df_plot2_long <- df_plot2 %>% 
  pivot_longer(cols = -time, names_to = 'dysplasia', values_to = 'surv')

plot2 <- ggplot(df_plot2_long, aes(x = time, y = 1 - surv, color = dysplasia)) + 
  geom_line(linewidth = 1) +
  scale_color_manual(values = color_palette[1:2]) +
  scale_x_continuous(limits = c(0, 5), breaks = seq(0, 5, 1)) +
  scale_y_continuous(limits = c(0, 1), breaks = seq(0, 1, 0.2)) +
  labs(title = "(B) Dysplasia", x = "Time to Recurrence (years)", y = "Probability of Recurrence", color = "Dysplasia") +
  theme_classic() +
  custom_theme +
  # Add table with placeholder values
  annotate("text", x = 0.2, y = 0.58, label = "                        HR      95% CI        p-value", hjust = 0, size = 4, fontface = "bold") +
  annotate("text", x = 0.2, y = 0.5, label = bquote("High grade      2.91     2.59, 3.27    "~7.06 %*% 10^{-72}), hjust = 0,  size = 4)

# For Number of Polyps
sur3 <- survfit(mod3, newdata = newdd3)
df_plot3 <- data.frame(time = sur3$time)
for (i in 1:3) {
  df_plot3[, i + 1] <- sur3$surv[, i]
}
names(df_plot3) <- c('time', polyp_numbers)
df_plot3_long <- df_plot3 %>% 
  pivot_longer(cols = -time, names_to = 'polyp_num', values_to = 'surv')

plot3 <- ggplot(df_plot3_long, aes(x = time, y = 1 - surv, color = polyp_num)) + 
  geom_line(linewidth = 1) +
  scale_color_manual(values = color_palette[1:3]) +
  scale_x_continuous(limits = c(0, 5), breaks = seq(0, 5, 1)) +
  scale_y_continuous(limits = c(0, 1), breaks = seq(0, 1, 0.2)) +
  labs(title = "(C) Number of Polyps", x = "Time to Recurrence (years)", y = "Probability of Recurrence", color = "Number of Polyps") +
  theme_classic() +
  custom_theme +
  # Add table with placeholder values
  annotate("text", x = 0.2, y = 0.58, label ="                     HR      95% CI         p-value", hjust = 0,  size = 4, fontface = "bold") +
  annotate("text", x = 0.2, y = 0.5, label =  bquote("≥3                 1.70     1.62, 1.79     "~2.53 %*% 10^{-88}), hjust = 0, size = 4) +
  annotate("text", x = 0.2, y = 0.45, label = bquote("Unknown      1.17    1.09, 1.27     "~4.94 %*% 10^{-5}), hjust = 0, size = 4)

# For Polyp Size
sur4 <- survfit(mod4, newdata = newdd4)
df_plot4 <- data.frame(time = sur4$time)
for (i in 1:3) {
  df_plot4[, i + 1] <- sur4$surv[, i]
}
names(df_plot4) <- c('time', polyp_sizes)
df_plot4_long <- df_plot4 %>% 
  pivot_longer(cols = -time, names_to = 'polyp_size', values_to = 'surv')

plot4 <- ggplot(df_plot4_long, aes(x = time, y = 1 - surv, color = polyp_size)) + 
  geom_line(linewidth = 1) +
  scale_color_manual(values = color_palette[1:3]) +
  scale_x_continuous(limits = c(0, 5), breaks = seq(0, 5, 1)) +
  scale_y_continuous(limits = c(0, 1), breaks = seq(0, 1, 0.2)) +
  labs(title = "(D) Polyp Size (mm)", x = "Time to Recurrence (years)", y = "Probability of Recurrence", color = "Polyp Size") +
  theme_classic() +
  custom_theme +
  # Add table with placeholder values
  annotate("text", x = 0.2, y = 0.58, label = "                   HR      95% CI        p-value", hjust = 0, size = 4, fontface = "bold") +
  annotate("text", x = 0.2, y = 0.5, label = bquote("≥10              1.65    1.56, 1.75    "~2.15 %*% 10^{-63}), hjust = 0,  size = 4) +
  annotate("text", x = 0.2, y = 0.45, label = bquote("Unknown     0.81    0.77, 0.85    "~8.77 %*% 10^{-18}), hjust = 0, size = 4)

# Combine and save plots
options(repr.plot.width = 18, repr.plot.height = 10)
combined_plot <- gridExtra::grid.arrange(
  plot1, plot2, plot3, plot4,
  ncol = 2
)

ggsave("Combined_Polyp_Survival_Analysis_5yr.png", combined_plot, width = 18, height = 11, dpi = 500)



# Set output to PDF file
cairo_pdf("Combined_Polyp_Survival_Analysis_5yr.pdf", width = 18, height = 11, family = "Arial Unicode MS")

# Draw the combined plot
grid.draw(combined_plot)

# Close the PDF device to save the file
dev.off()

